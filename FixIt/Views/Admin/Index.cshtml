@model AdminViewModel
@{
    ViewData["Title"] = "Index";
}

<h1>Administrator page</h1>
<h3>Manage your things here</h3>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
       @foreach (var user in Model.Administrators)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
                
            </tr>
        }
    </tbody>

    <thead>
        <tr>
            <td>Id</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
        @************************************************************************@
        @foreach (var user in Model.Everyone)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>

            </tr>
        }
    </tbody>
</table>